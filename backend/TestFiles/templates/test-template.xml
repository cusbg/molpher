
<!--
    Template can be loaded instead of snapsthot at any place.
    Molper distinguish between snp a template file and use
    proper loading function. The file must end on '-template.xml'
    if Molpher should interpreate file as a template.
    If file ends only on 'xml' Molpher try to load it as a regular
    xml-snapstho file wich will almost surely failed.

    When using templates Molpher write info about loaded into
    snapshost. For information about other snaphost deafult values
    please refer to commonst/iteration_snaphost.hpp.

    The <fingerprint> respectively <similarity> should use
    values from enums in common/fingerprintSelector.hpp
    respectively common/simCoefSelector.hpp
-->
<iteration>
  <inputActivityDataDir>../../FS/FeatSele/data/</inputActivityDataDir>
  <allActivesSMILES>../../FS/FeatSele/data/aid466_actives_all.smi</allActivesSMILES>
  <activesDescriptors>../../FS/FeatSele/data/aid466_actives.csv</activesDescriptors>
  <testActivesDescriptors>../../FS/FeatSele/data/aid466_actives_muv.csv</testActivesDescriptors>
  <inactivesDescriptors>../../FS/FeatSele/data/aid466_decoys.csv</inactivesDescriptors>
  <descriptorWeights>../../FS/FeatSele/FS_outputs/aid466_FS4_weights.csv</descriptorWeights>
    
  <fingerprint>FP_MORGAN</fingerprint>
  <similarity>SC_TANIMOTO</similarity>
  <saveDataAsCSVs>true</saveDataAsCSVs>
    <param>
	<substructureRestriction>false</substructureRestriction>
	<syntetizedFeasibility>true</syntetizedFeasibility>
	
	<activityMorphing>true</activityMorphing>
	<startMolMaxCount>2</startMolMaxCount>
	<farProduce>10</farProduce>
	<iterMax>20</iterMax>
	<maxTimeMinutes>240</maxTimeMinutes>
	<maxMOOPruns>1</maxMOOPruns>
	<nonProducingSurvive>2</nonProducingSurvive>
        <itersUntilDecay>3</itersUntilDecay>
	<maxMorhpsTotal>2000</maxMorhpsTotal>
	<etalonType>mean</etalonType>
	<!--this is turned off <maxAcceptableEtalonDistance>10000</maxAcceptableEtalonDistance> -->
    </param>
</iteration>
